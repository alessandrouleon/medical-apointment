{
    "openapi": "3.0.0",
    "info": {
        "title": "Serviço de agendamento de consulta",
        "description": "API responsavel pelo agendamento de consulta",
        "vesion": "1"
    },
    "paths": {
        "/speciality": {
            "post": {
                "tags": [
                    "Speciality"
                ],
                "description": "Cadastro de especialidade",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ISpecialityRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ISpecialityResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ISpecialityRequest": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Nome da especialidade",
                        "example": "NAME_TEST"
                    },
                    "description": {
                        "type": "string",
                        "description": "Descrição da especialidade",
                        "example": "DESCRIPTION_TEST"
                    }
                },
                "required": [
                    "name",
                    "description"
                ]
            },
            "ISpecialityResponse": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Nome da especialidade",
                        "example": "NAME_TEST"
                    },
                    "description": {
                        "type": "string",
                        "description": "Descrição da especialidade",
                        "example": "DESCRIPTION_TEST"
                    },
                    "id": {
                        "type": "string",
                        "description": "ID da Especialidade",
                        "format": "uuid"
                    },
                    "createdAt": {
                        "type": "string",
                        "description": "Data da criação da especialidades",
                        "format": "date-time"
                    }
                }
            }
        }
    }
}